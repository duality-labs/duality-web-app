  "lint-staged": {
    "**/*.{mjs,js,jsx,ts,tsx,json,css,scss,html,md,yaml,yml}": [
      "prettier --write"
    ],
    "**/*.{mjs,js,jsx,ts,tsx}": [
      "eslint --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie 11",
      "not ie < 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
