    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^28.1.5",
    "@types/node": "^16.11.27",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/seedrandom": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
