    "chain-registry": "^1.20.0",
    "invariant": "^2.2.4",
    "long": "^5.2.3",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
